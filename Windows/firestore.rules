rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  
  	match /users/{userId} {
    	allow read, write;
    }
  
    match /chats/{document=**} {
      allow read, write: if request.auth != null;
          // request.time < timestamp.date(2022, 3, 30);
    }
    
    
    
  }
}